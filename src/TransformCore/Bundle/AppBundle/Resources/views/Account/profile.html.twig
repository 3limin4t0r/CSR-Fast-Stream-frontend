{% extends "::base.html.twig" %}

{% block title %}
    {% trans %}profile.title{% endtrans %}
{% endblock %}

{% block breadcrumb %}
    <div class="row ">
        <ol class="breadcrumb">
            <li><a href="{{ path('transform_core_app_account') }}">{% trans %}account.title{% endtrans %}</a></li>
            <li class="active">{% trans %}profile.title{% endtrans %}</li>
            <li><a href="{{ path('transform_core_app_eligibility') }}">{% trans %}eligibility.title{% endtrans %}</a></li>
        </ol>
    </div>
{% endblock %}

{% block body %}

    <div class="row">

        <div class="col-md-12">
            <h2>{% trans %}profile.title{% endtrans %}</h2>
        </div>

    </div>

    <div class="row">

        <div class="col-md-12">
            {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}

                {{ form_widget(form) }}

            {{ form_end(form) }}
        </div>

    </div>

{% endblock %}
