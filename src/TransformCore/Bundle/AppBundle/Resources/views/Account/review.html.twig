{% extends "::base.html.twig" %}

{% block title %}
    {% trans %}review.title{% endtrans %}
{% endblock %}

{% block breadcrumb %}

    <div class="row ">
        <ol class="breadcrumb">
            <li><a href="{{ path('transform_core_app_account') }}">{% trans %}account.title{% endtrans %}</a></li>
            <li><a href="{{ path('transform_core_app_profile_edit') }}">{% trans %}profile.title{% endtrans %}</a></li>
            <li><a href="{{ path('transform_core_app_eligibility') }}">{% trans %}eligibility.title{% endtrans %}</a></li>
            <li class="active">{% trans %}review.title{% endtrans %}</li>
        </ol>
    </div>

{% endblock %}

{% block body %}

    <div class="row">

        <div class="col-md-12">
            <h2>{% trans %}review.title{% endtrans %}</h2>
        </div>

    </div>

    <div class="row">

        <div class="col-md-12">
            <h2>{% trans %}profile.title{% endtrans %}</h2>
        </div>

        <div class="col-md-12">
            <h2>{% trans %}eligibility.title{% endtrans %}</h2>
        </div>

    </div>

{% endblock %}
